{
  "name": "fastify-service-template",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "copy:config": "cpy --rename=.env ./.env.default ./",
    "test": "jest --config=jest.config.json",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "test:ci": "npm run copy:config && npm run lint && npm run test:coverage",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --write \"{src,test}/**/*.{js,ts}\" src/app.ts",
    "start:dev": "ts-node src/server.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "awilix": "^5.0.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "fastify": "^3.22.1",
    "fastify-auth": "^1.1.0",
    "fastify-awilix": "^1.2.0",
    "fastify-cors": "^6.0.2",
    "fastify-healthcheck": "^3.1.0",
    "fastify-helmet": "^5.3.2",
    "fastify-jwt": "^3.2.1",
    "fastify-no-icon": "^4.0.0",
    "fastify-swagger": "^4.12.6",
    "fastq": "^1.13.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.95.12",
    "knex-repositories": "^3.1.2",
    "knex-utils": "^5.4.0",
    "pg": "^8.7.1",
    "pino": "^7.0.5",
    "tiny-lru": "^7.0.6",
    "ts-node": "^10.4.0",
    "validation-utils": "^7.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "knex-tablecleaner": "^5.0.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.4.4"
  }
}
