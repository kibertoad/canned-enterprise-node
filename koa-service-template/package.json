{
  "name": "koa-service-template",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "test:placeholder": "jest --config=jest.config.json",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "test:ci:placeholder": "npm run lint && npm run test:coverage",
    "lint": "eslint --format codeframe \"src/**/*.ts\" \"app.ts\"",
    "prettier": "prettier --write \"{src,test}/**/*.{js,ts}\" app.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "api-contract-validator": "^1.3.0",
    "express-ajv-swagger-validation": "^1.1.3",
    "koa": "^2.11.0",
    "koa-router": "^8.0.8",
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/koa": "^2.11.2",
    "@types/koa-router": "^7.4.0",
    "@types/node": "^12.12.30",
    "@types/superagent": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nock": "^12.0.3",
    "nockback-harder": "^5.0.0",
    "prettier": "^1.19.1",
    "swagger-combinator": "^1.0.1",
    "ts-jest": "^25.2.1",
    "typescript": "3.8.3"
  }
}
