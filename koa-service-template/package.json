{
  "name": "koa-service-template",
  "version": "1.0.0",
  "scripts": {
    "build": "del-cli dist/ && tsc",
    "test": "jest --config=jest.config.json",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "test:ci": "npm run eslint && npm run test:coverage",
    "eslint": "eslint --format codeframe \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\" \\\"app.ts\\\"",
    "prettier": "prettier --write \"{src,test}/**/*.{js,ts}\" app.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "api-contract-validator": "^1.2.6",
    "express-ajv-swagger-validation": "^1.0.7",
    "koa": "^2.8.1",
    "koa-router": "^7.4.0",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/koa": "^2.0.49",
    "@types/koa-router": "^7.0.42",
    "@types/node": "^10.14.17",
    "@types/superagent": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "del-cli": "^2.0.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "nock": "^10.0.6",
    "nockback-harder": "^3.0.1",
    "prettier": "^1.18.2",
    "swagger-combinator": "^1.0.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  }
}
