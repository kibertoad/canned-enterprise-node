{
  "name": "javascript-template",
  "version": "1.0.0",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "index.js",
  "scripts": {
    "test": "jest --config=jest.config.json",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "test:ci": "npm run eslint && npm run test:coverage",
    "eslint": "eslint --format codeframe \"lib/**/*.js\" \"test/**/*.js\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\" index.js"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 75,
        "functions": 90,
        "lines": 85
      }
    }
  },
  "homepage": "http://github.com/kibertoad/canned-enterprise-node",
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/canned-enterprise-node.git"
  },
  "keywords": [
    "javascript",
    "template"
  ],
  "files": [
    "README.md",
    "LICENSE"
  ]
}
